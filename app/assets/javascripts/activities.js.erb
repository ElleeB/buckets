function toDo(name, activity, user) {
	this.name = name,
  this.activity = activity,
	this.user = user,
	this.headline = function() {
		return this.name.toUpperCase() + " | " + this.activity.toUpperCase() + " | " + this.user.toUpperCase();
	}
}

